class Solution {
    public int missingNumber(int[] nums) {
        int arr[] = new int[nums.length + 1]; 
        int k = -1;
        for (int i = 0; i < nums.length; i++) {
            arr[nums[i]]++;
        }
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] == 0) {
                k = i;
                break;
            }
        }       
        return k;
    }
}
